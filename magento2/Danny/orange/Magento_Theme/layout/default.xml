<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="page-background">
            
        </referenceContainer>
        
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">images/logo.png</argument>
                <argument name="logo_img_width" xsi:type="string">auto</argument>
                <argument name="logo_img_height" xsi:type="string">auto</argument>
            </arguments>
        </referenceBlock>
        
        <!-- <referenceBlock name="catalog.topnav"  remove="true"/> -->

        <move element="navigation.sections" name="ves.topmenu" destination="ves_header" as="catalog.topnav" remove="true"/>

        <move element="skip_to_content" as="skipToContent" destination="ves_header" remove="true"/>
        <move element="minicart" destination="ves_header" remove="true"/>
        <move element="top.search" destination="ves_header" remove="true"/>
        <!-- <move element="logo" as="logo" destination="ves_header"/> -->
        <move element="top.links" as="toplinks" destination="ves_header" remove="true"/>

        <referenceContainer name="header.panel.wrapper" remove="true"/>

        <referenceBlock name="report.bugs" remove="true"/>
        <!-- Footer section showing copyright -->
        <referenceContainer name="footer-container" remove="true"/>
        <!-- Modals-wrapper popup to sign -->
        <referenceContainer name="after.body.start" remove="true"/>
        <!-- Breadcrumbs section showing Home -> 24 Hour Health -->
        <block class="Magento\Theme\Block\Html\Breadcrumbs" name="breadcrumbs" as="breadcrumbs" remove="true"/>

    </body>
</page>


